{
    "name": "MY Annotation",
    "version": "1.0",
    "description": "based on base",
    "browser_action": {
        "default_icon": "icon-34.png",
        "default_title": "annotation",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions" : ["contextMenus", "activeTab"],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["contentScript.js"]
        }
    ],
    "manifest_version": 2
}